<div class = "main-body">
    {{add-code flag="secondarySchool"}}
    <br>
    <h3>Current Secondary Schools</h3>
    <table class="student-table">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>
      {{#each model as |oneSecondarySchool|}}
      <tr>
        <td >
          <div class="large-6-columns">
            {{oneSecondarySchool.name}}
          </div>
          <div></div>
        </td>
        <td>
          {{edit-code flag="secondarySchool" id=oneSecondarySchool.id}}
        </td>
      </tr>
      {{/each}}
    </table><br>
</div>
